{
  "name": "apm-communication",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build --progress --verbose  --common-chunk --vendor-chunk --extract-css --progress --named-chunks",
    "build-watch": "ng build --progress --verbose --vendor-chunk --extract-css --named-chunks --watch",
    "build-prod": "ng build --progress --verbose --common-chunk --vendor-chunk --extract-css --named-chunks --prod --output-hashing=none --progress",
    "start": "ng serve --open --vendor-chunk --extract-css --common-chunk --named-chunks --output-hashing=none --progress ",
    "start-prod": "ng serve --open --vendor-chunk --extract-css --common-chunk --named-chunks --prod --aot --output-hashing=none --progress",
    "test": "ng test",
    "lint": "ng lint --typeCheck --format stylish",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "bootstrap": "^4.0.0",
    "core-js": "^2.0.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.0.0",
    "popper.js": "^1.0.0",
    "rxjs": "^5.0.0",
    "tether": "^1.0.0",
    "zone.js": "^0.8.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.1",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "^2.0.0",
    "@types/jasminewd2": "^2.0.0",
    "@types/node": "^6.0.0",
    "angular-in-memory-web-api": "^0.5.0",
    "codelyzer": "^4.0.0",
    "jasmine-core": "^2.0.0",
    "jasmine-spec-reporter": "^4.0.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-jasmine": "^1.0.0",
    "karma-jasmine-html-reporter": "^0.2.0",
    "protractor": "^5.0.0",
    "ts-node": "~4.1.0",
    "tslint": "^5.0.0",
    "typescript": "^2.7.0"
  }
